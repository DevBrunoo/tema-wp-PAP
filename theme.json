{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#131313",
					"name": "Black",
					"slug": "pap-black"
				},
				{
					"color": "#8B8B8B",
					"name": "Gray",
					"slug": "pap-gray"
				},
				{
					"color": "#f7f7f7",
					"name": "White",
					"slug": "pap-white"
				}
			]
		},
		"custom": {
			"line-height": {
				"medium": 1.4,
				"normal": 1.6,
				"small": 1.3,
				"tiny": 1.15
			},
			"typography": {
				"font-size": {
					"heading-1": "clamp(2113rem, 1925.2rem + 939vw, 3052rem)",
					"heading-2": "clamp(1.699rem, 1.5506000000000002rem + 0.7419999999999998vw, 2.441rem)",
					"heading-3": "clamp(1.35rem, 1.2294rem + 0.603vw, 1.953rem)",
					"heading-4": "clamp(1.253rem, 1.1909999999999998rem + 0.31000000000000005vw, 1.563rem)",
					"heading-5": "clamp(1.063rem, 1.0255999999999998rem + 0.18700000000000006vw, 1.25rem)",
					"heading-6": "clamp(1.063rem, 1.0255999999999998rem + 0.18700000000000006vw, 1.25rem)"
				}
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1183px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "'Inter', sans-serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/inter/inter-Light.ttf"
							]
						},
						{
							"fontFamily": "'Inter', sans-serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/inter/inter-Regular.ttf"
							]
						}
					],
					"fontFamily": "'Inter', sans-serif",
					"name": "Body - Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "'Tilt Neon', 'cursive'",
					"name": "Headings - Neon",
					"slug": "TiltNeon"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.64rem",
					"slug": "small"
				},
				{
					"name": "Small",
					"size": "0.8rem",
					"slug": "medium"
				},
				{
					"name": "Regular",
					"size": "1rem",
					"slug": "regular"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"color": "currentColor",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--pap-black)",
					"text": "var(--wp--preset--color--pap-white)"
				},
				"spacing": {
					"padding": {
						"bottom": "0.7em",
						"left": "1.6em",
						"right": "1.6em",
						"top": "0.7em"
					}
				},
				"typography": {
					"fontSize": "va(--wp--prset--font-size--regular)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"core/latest-posts": {
				"spacing": {
					"padding": {
						"bottom": "20px"
					}
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "32px",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontWeight": "800",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--pap-black)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset-font-family--inter)",
			"fontSize": "1rem",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}